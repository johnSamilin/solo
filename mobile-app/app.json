{
  "expo": {
    "name": "solo",
    "slug": "solo",
    "version": "1.2.0",
    "icon": "../assets/icons/png/1024x1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.johnsamilin.solo.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../assets/icons/png/1024x1024.png",
        "backgroundColor": "#ffffff"
      },
      "package": "net.johnsamilin.solo.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "../assets/icons/png/1024x1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "expoProvidesDefaultConfig": true
          }
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}