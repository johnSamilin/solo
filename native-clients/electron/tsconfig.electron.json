{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "outDir": "./electron/dist",
    "rootDir": "./electron",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "types": ["node"],
    "typeRoots": ["./electron", "./node_modules/@types"]
  },
  "include": ["electron/**/*"],
  "exclude": ["node_modules"]
}
